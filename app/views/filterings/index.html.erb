<p id="notice"><%= notice %></p>

<h1>Filterings</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @filterings.each do |filtering| %>
      <tr>
        <td><%= link_to 'Show', filtering %></td>
        <td><%= link_to 'Edit', edit_filtering_path(filtering) %></td>
        <td><%= link_to 'Destroy', filtering, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Filtering', new_filtering_path %>
