<p id="notice"><%= notice %></p>

<h1>Imputation Methods</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @imputation_methods.each do |imputation_method| %>
      <tr>
        <td><%= link_to 'Show', imputation_method %></td>
        <td><%= link_to 'Edit', edit_imputation_method_path(imputation_method) %></td>
        <td><%= link_to 'Destroy', imputation_method, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Imputation Method', new_imputation_method_path %>
